                              %legend
                                Events
                                %span#event_cloning_controls.pull-right
                                  %span#event_cloning_add.btn.btn-mini
                                    %i.icon-plus
                              #event_cloning_template.row-fluid.sheepit-form
                                .control-group.clearfix
                                  .span6
                                    %label.control-label{for: "event_cloning_#index#_ename"} Name
                                    .controls
                                      %input.span12{id: "event_cloning_#index#_ename", name: "ename[#index#]", type: "text"}/
                                  .span6
                                    %label.control-label{for: "event_cloning_#index#_ename"} Date
                                    .controls.controls-row
                                      %input.span6.datepicker_s{id: "event_cloning_#index#_estart", name: "estart[#index#]", type: "text"}/
                                      %input.span6.datepicker_e{id: "event_cloning_#index#_eend", name: "eend[#index#]", type: "text"}/
                                  %span#event_cloning_remove_current.close ×
                              #event_cloning_noforms_template.control-group
