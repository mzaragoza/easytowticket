                #main-content
                  .row-fluid
                    .span6
                      .widget
                        .widget-header
                          %span.title Basic Input Cloning
                        .widget-content.form-container
                          %form.form-horizontal
                            %fieldset#input_cloning.sheepit-form
                              %legend
                                Input Cloning
                                %span#input_cloning_controls.pull-right
                                  %span#input_cloning_add.btn.btn-mini
                                    %i.icon-plus
                              #input_cloning_template.control-group
                                %label.control-label{for: "input_cloning_#index#_input"}
                                  Text Input
                                  %span#input_cloning_label
                                .controls
                                  %input{id: "input_cloning_#index#_input", name: "text[#index#]", type: "text"}/
                                %span#input_cloning_remove_current.close ×
                              #input_cloning_noforms_template.control-group
                                %p.help-block Add a new input by clicking the (+) button above
                            .form-actions
                              %button.btn.btn-primary{type: "submit"} Save changes
                              %button.btn{type: "reset"} Cancel
                    .span6
                      .widget
                        .widget-header
                          %span.title Optional Input Cloning
